/**index.wxss**/

.track-bg {
  background-color: #f8f8f8;
  width: 100%;
  text-align: center;
  height: 20vh;
  background: url(data:image/png;base64,);
}
 
.track-bg .logo {
  width: 35%;
  margin-bottom: 50rpx;
  margin-top: 70rpx;
}

.track-input-block {
  position: relative;
  margin-bottom: 50rpx;
  margin-top: 70rpx;
}

.track-input-block icon {
  position: absolute;
  left: 30rpx;
  top: 22rpx;
}

.track-input {
  box-shadow: inset 0 2rpx 2rpx rgba(0, 0, 0, 0.075), 0 0 16rpx #ccc;
  background-color: #fff;
  border: 1px solid #ccc;
  width: 60%;
  border-radius: 12rpx;
  font-size: 24rpx;
  margin: 0rpx 30rpx 0rpx 10rpx;
  height: 70rpx;
  text-align: left;
  padding: 0rpx 30rpx 0rpx 60rpx;
}

.track-history {
  width: 100%;
  height: 80vh;
  left: 0;
  bottom: 0;
}

.track-history-title {
  font-size: 24rpx;
  color: #999;
  margin-bottom: 20rpx;
}

.track-history-item {
  border: 2rpx solid #ddd;
  border-radius: 8rpx;
  box-shadow: 0 2rpx 2rpx rgba(0, 0, 0, 0.05);
  font-size: 24rpx;
  margin-bottom: 30rpx;
}

.track-history-item-title {
  padding: 20rpx;
  color: #333;
  background-color: #f5f5f5;
  border-color: #ddd;
  border-top-left-radius: 6rpx;
  border-top-right-radius: 6rpx;
}

.track-history-item-title icon {
  float: left;
  overflow: hidden;
  margin-top: 0;
}

.track-history-item-title view {
  margin-left: 80rpx;
  min-height: 62rpx;
}

.track-history-item-title text {
  display: block;
}

.track-history-item-title .track-number {
  font-size: 28rpx;
  color: #31708f;
  font-weight: bold;
  margin-bottom: 10rpx;
}

.track-history-item-content {
  padding: 20rpx;
}

.track-history-item-content text {
  display: block;
}

.track-success {
  border-color: #d6e9c6;
}

.track-success .track-history-item-title {
  background-color: #dff0d8;
}

.track-error {
  border-color: #ebccd1;
}

.track-error .track-history-item-title {
  background-color: #f2dede;
}

.track-info {
  border-color: #bce8f1;
}

.track-info .track-history-item-title {
  background-color: #d9edf7;
}

.track-null {
  border-color: #ddd;
}

.track-null .track-history-item-title {
  background-color: #f5f5f5;
}

.i-scan {
  height: 70rpx;
  position: absolute;
  right: 40rpx;
  top: 0rpx;
  font-size: 30rpx;
  margin: 0rpx 20rpx 20rpx 20rpx;
}
